type Stats = 
 record {
   data: vec CanisterData;
   time: text;
 };
type PriceData = 
 record {
   date: text;
   price: float64;
 };
type CanisterData = 
 record {
   canisterId: text;
   price: float64;
 };
service : {
  addStats: (Stats) -> ();
  getPrice: () -> (PriceData) query;
  getStat: (text) -> (opt Stats) query;
  getStats: () -> (vec Stats) query;
  updatePrice: (PriceData) -> (PriceData);
}
